<launch>

  <arg name="scan_topic" default="$(eval optenv('JACKAL_LASER_TOPIC', 'front/scan'))" />

  <!--- Run gmapping -->
  <include file="$(find jackal_navigation_mark)/launch/include/gmapping.launch">
    <arg name="scan_topic"    value="$(arg scan_topic)" />
  </include>


  <!--- Run Move Base -->
  <include file="$(find jackal_navigation_mark)/launch/include/move_base.launch" />

</launch>
